<view class="page">
    <view class="info">
        <view class="title">当前小程序cookie：</view>
        <view class="content">{{content}}</view>
    </view>

    <view class="button-container">
        <button type='primary' bindtap="setCookie1" size='large'>设置setCookie1/2</button>
        <button type='primary' bindtap="setCookie2" size='large'>设置setCookie3/4</button>
        <button type='primary' bindtap="removeCookie">清除removeCookie2</button>
        <button type='primary' bindtap="lunchRequest">发起请求(请求头/响应头 cookie处理)</button>
    </view>
</view>